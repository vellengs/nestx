{
  "name": "nest-swagger",
  "version": "0.4.0",
  "description": "generate swagger for nestjs",
  "main": "./dist/swagger/decorators.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier lib/**/*.ts --write",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public"
  },
  "dependencies": {
    "argparse": "^1.0.10",
    "lodash": "4.17.11",
    "merge": "^1.2.1",
    "path-to-regexp": "3.0.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@nestjs/common": "6.0.1",
    "@nestjs/core": "6.0.1",
    "@nestjs/platform-express": "^9.2.1",
    "@types/argparse": "^1.0.36",
    "@types/mocha": "^5.2.6",
    "@types/node": "11.11.3",
    "chai": "^4.2.0",
    "husky": "0.14.3",
    "lint-staged": "8.1.5",
    "mocha": "^6.0.2",
    "prettier": "1.16.4",
    "reflect-metadata": "0.1.13",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.4000",
    "typescript-rest": "^2.0.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add -f"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vellengs/nestx.git"
  },
  "keywords": [
    "nestjs",
    "swagger"
  ],
  "author": "vellengs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vellengs/nestx/issues"
  },
  "homepage": "https://github.com/vellengs/nestx#readme",
  "bin": {
    "swaggerGen": "dist/cli.js"
  },
  "directories": {
    "lib": "dist",
    "doc": "doc"
  },
  "gitHead": "3f01311f987fc4f02f1e64f74365d53780a0a497"
}
