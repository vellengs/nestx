  "scripts": {
    "clean": "rimraf ./dist",
    "analyze": "ng build --prod --build-optimizer --stats-json",
    "build": "npm run clean && ng build --prod --build-optimizer",
    "precommit": "npm run lint-staged",
    "e2e": "ng e2e",
    "lint": "run-s lint:ts lint:style",
    "lint-staged": "lint-staged",
    "lint:style": "stylelint \"{src}/**/*.less\" --syntax less",
    "lint:ts": "ng lint",
    "ng": "ng",
    "serve:hmr": "ng serve -o --hmr -e=hmr",
    "build:config": "tsc -p tsconfig.appearances.json",
    "start": "npm run color-less && ng serve -o --port 4700 --proxy-config proxy.conf.json",
    "test": "ng test -sr -sm=false",
    "test-coverage": "ng test -sr -cc -sm=false",
    "color-less": "node scripts/color-less.js",
    "build-copy": "ts-node build.copy.ts"
  },
